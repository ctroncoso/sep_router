<h1>Carga de Pacientes para</h1>
<h2>Examen <%=pluralize @prestacion.descripcion%></h2>


<table>
  <tr>
    <th>Examen</th>
    <th>ID</th>
    <th></th>
    <th></th>
  </tr>

<% @pacientes.each do |paciente| %>
  <tr>
    <td><%= link_to paciente.name, patient_exams_path(paciente)%></td>
    <td><%= paciente.id%></td>
  </tr>
<% end %>
</table>
